    "jest": "^22.4.3",
    "nodemon": "^1.18.3",
    "regenerator-runtime": "^0.12.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "/tests/.*\\.test\\.js$",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
